{
  "name": "insightextractor-server",
  "version": "1.0.0",
  "description": "Backend server for InsightExtractor",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "check-db": "ts-node src/scripts/checkDatabase.ts",
    "netlify:build": "npm run build",
    "netlify:serve": "netlify-lambda serve src",
    "netlify:build-server": "netlify-lambda build src"
  },
  "keywords": [
    "insights",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "mongoose": "^8.3.1",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.1",
    "netlify-lambda": "^2.0.16",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
